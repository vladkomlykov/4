<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pizzadelivery.CartController">
   <children>
      <Label layoutX="248.0" layoutY="37.0" text="Корзина" textFill="#fffefe">
